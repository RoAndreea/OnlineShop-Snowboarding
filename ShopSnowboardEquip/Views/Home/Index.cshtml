@model HomeViewModel

@{
    ViewData["Title"] = "Home Page";
}

@await Html.PartialAsync("Carousel")
<h2>Prefered equipments of the week </h2>
<h4>Clients preferences - just for you!</h4>

<div class="row">
    @foreach (var equipment in Model.PreferredEquipments)
    {
        @Html.Partial("EquipmentSummary", equipment)
    }
</div>


